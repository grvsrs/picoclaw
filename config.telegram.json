{
  "agents": {
    "defaults": {
      "workspace": "~/.picoclaw/workspace",
      "model": "moonshot-v1-32k",
      "max_tokens": 8192,
      "temperature": 0.7,
      "max_tool_iterations": 20
    }
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "token": "YOUR_TELEGRAM_BOT_TOKEN_HERE",
      "allow_from": [
        "YOUR_TELEGRAM_USER_ID"
      ]
    }
  },
  "providers": {
    "moonshot": {
      "api_key": "sk-kimi-YOUR_KEY_HERE",
      "api_base": ""
    }
  },
  "tools": {
    "web": {
      "search": {
        "api_key": "",
        "max_results": 5
      }
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 18790
  }
}
